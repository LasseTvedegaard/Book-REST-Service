{
  "ConnectionStrings": {
    "DbAccessConnection": "Data Source=.;Database=BookLibrary;integrated security=true;"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Users\\lasse\\source\\repos\\BookBuddyProject2\\Book-REST-Service-master\\Book-REST-Service\\Logs\\log-.txt",
          "rollingInterval": "Day",
          "RollingFileSizeLimit": true,
          "retainedFileCountLimit": 31,
          "formatter": "Serilog.Formatting.Json.JsonFormatter"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "frontend_url": "http://localhost:3000"
}
